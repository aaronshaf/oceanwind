{
  "name": "oceanwind",
  "version": "0.2.2",
  "description": "compiles tailwind like shorthand syntax into css at runtime",
  "module": "index.js",
  "main": "index.js",
  "scripts": {
    "start": "servor --browse --reload --editor",
    "build": "esbuild --bundle index.js --outfile=index.min.js --format=esm --minify",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "keywords": [],
  "author": "Luke Jackson (@lukejacksonn)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "babel-jest": "^26.3.0",
    "esbuild": "0.6.28",
    "jest": "^26.4.2",
    "servor": "4.0.2"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  }
}
